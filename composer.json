{
    "name": "trappar/alice-generator",
    "description": "Automatically generates alice fixture based on a set of objects",
    "keywords": ["alice", "fixture", "data", "test", "generator", "orm", "doctrine"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Way",
            "email": "jeff.way@me.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/yaml": "^2.8||^3.0|^4.0",
        "doctrine/common": "^2.6",
        "jms/metadata": "^1.5||^2.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.6.3",
        "phpunit/phpunit": "^7.1",
        "nelmio/alice": "^3.3",
        "phpstan/phpstan": "^0.11.7",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-phpunit": "^0.11.2"
    },
    "autoload": {
        "psr-4": { "Trappar\\AliceGenerator\\": "src/Trappar/AliceGenerator" }
    },
    "autoload-dev": {
        "psr-4": { "Trappar\\AliceGenerator\\Tests\\": "tests/Trappar/AliceGenerator" }
    },
    "scripts": {
        "analyze": "phpstan analyze -c phpstan.neon.dist --no-progress"
    }
}
